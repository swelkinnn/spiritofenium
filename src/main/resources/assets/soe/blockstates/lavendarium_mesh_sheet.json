        "west": true
      },
      "apply": {
        "model": "soe:block/lavendarium_mesh_sheet_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "soe:block/lavendarium_mesh_sheet_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "soe:block/lavendarium_mesh_sheet_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "soe:block/lavendarium_mesh_sheet_noside_alt",
        "y": 90
      }
