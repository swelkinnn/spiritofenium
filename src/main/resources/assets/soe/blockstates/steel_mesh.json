{
  "multipart": [
    {
      "apply": {
        "model": "soe:block/steel_mesh_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "soe:block/steel_mesh_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "soe:block/steel_mesh_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "soe:block/steel_mesh_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "soe:block/steel_mesh_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "soe:block/steel_mesh_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "soe:block/steel_mesh_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "soe:block/steel_mesh_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "soe:block/steel_mesh_noside",
        "y": 270
      }
    }
  ]
}